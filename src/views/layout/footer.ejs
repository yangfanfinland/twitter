    <script>
        $(function() {        
            // Logout
            $('#btn-logout').click(function() {
                if (confirm('Confirm logout?') === false) {
                    return
                }
                ajax.post('/api/user/logout', function(err, data) {
                    if (err) {
                        console.error(err)
                        return
                    }
                    location.href = '/login'
                })
            })
        })
        </script>
